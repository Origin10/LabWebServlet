<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Pets.ExchangeStuff.model.Area" table="Area" schema="dbo" catalog="EEIT94">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="parentId">
            <column name="parent_id" sql-type="bigint"/>
        </property>
        <property name="common">
            <column name="common" sql-type="int"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(16)" length="16"/>
        </property>
    </class>
</hibernate-mapping>