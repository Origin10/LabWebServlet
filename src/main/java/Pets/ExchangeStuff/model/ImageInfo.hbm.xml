<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Pets.ExchangeStuff.model.ImageInfo" table="ImageInfo" schema="dbo" catalog="EEIT94">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="width">
            <column name="width" sql-type="int" not-null="true"/>
        </property>
        <property name="height">
            <column name="height" sql-type="int" not-null="true"/>
        </property>
        <property name="sourcePath">
            <column name="source_path" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>